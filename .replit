modules = ["nodejs-20"]

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Server"

[[workflows.workflow]]
name = "Server"
author = "agent"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm start"

[deployment]
run = ["npm", "install", "&&", "npm", "start"]
deploymentTarget = "vm"

[agent]
expertMode = true

